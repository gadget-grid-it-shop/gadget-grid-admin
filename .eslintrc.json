{
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "@typescript-eslint/no-unused-vars": [
      "warn", // Changed from "error" to "warn"
      {
        "argsIgnorePattern": "^_",
        "args": "none"
      }
    ],
    "@typescript-eslint/no-explicit-any": 0,
    "react/no-unescaped-entities": 0,
    "no-console": "warn"
  }
}
